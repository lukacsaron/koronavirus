            <div class="col-lg-4 col-md-12">

                <div class="col-12"
                    style="background-color: #445175; width: inherit; border-radius: 25px; box-shadow: 0 0 30px rgb(58, 68, 100); margin-bottom: 25px;">
                    <p
                        style="color: #DFDFEF; font-size: 30px; text-align: center; padding-top: 15px; font-weight: bolder;">
                        Kína</p>
                    <table>
                        <% for( let index = 0; index < mainlandData.length; index++ ) { %>
                        <tr>
                            <td style="color: rgb(205, 205, 224);"><span
                                    style="font-weight: bold; color: #DFDFEF; font-size: 18px;"><%= mainlandData[index][0] %></span><br>
                                Diagnosztizált: <span style="color: rgb(101, 221, 155);"><%= mainlandData[index][1] %></span>
                                <% if (mainlandData[index][3][0] > 0) { %>
                                Súlyos állapotban: <span style="color: rgb(248, 245, 64);"><%= mainlandData[index][3][0] %></span>
                                <% } %> <br>
                                <% if (mainlandData[index][3][1] > 0) { %>
                                Kritikus állapotban: <span style="color: #FE8943;"><%= mainlandData[index][3][1] %></span>
                                <% } %>
                                <% if (mainlandData[index][2] > 0) { %>
                                Halott: <span style="color: #F65164;"><%= mainlandData[index][2] %></span>
                                <% } %>
                                <a style="color: #DFDFEF;"
                                    href=<%= mainlandChinaSources[index] %>><span><u>Forrás</u></span></a>
                            </td>
                        </tr>
                        <% } %>
                    </table>
                </div>
            </div>
            <div class="col-lg-4 col-md-12">
